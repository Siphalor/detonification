{
  "schemaVersion": 1,
  "id": "detonification",
  "version": "0.6.0",

  "name": "Detonification",
  "description": "A mod to prevent breaking worlds when upgrading without ecotones. This means converting biomes to similar vanilla biomes but keeping the items and blocks.",
  "authors": [
    "SuperCoder79",
	"Siphalor"
  ],
  "contact": {
    "sources": "https://github.com/Siphalor/detonification"
  },

  "license": "LGPLv3",
  "icon": "assets/ecotones/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "supercoder79.ecotones.Detonification"
    ],
    "client": [
      "supercoder79.ecotones.EcotonesClient"
    ]
  },
  "mixins": [
    "ecotones.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.2",
    "fabric": "*",
    "minecraft": ">=1.16-"
  },
  "breaks": {
    "ecotones": "*"
  }
}
